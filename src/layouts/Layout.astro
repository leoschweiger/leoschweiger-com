---
import Header from "../components/Header.astro";
import Footer from "../components/Footer.astro";

const { title } = Astro.props;
const { ogImage } = Astro.props;
const { ogDesc: ogDesc } = Astro.props;
const { topId } = Astro.props;
---

<Header title={title} image={ogImage} topId={topId} ogDesc={ogDesc} />

<slot />

<Footer getInTouch={Astro.props.getInTouch} />
